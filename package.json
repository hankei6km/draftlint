{
  "name": "draftlint",
  "version": "0.1.0",
  "scripts": {
    "api:build": "aspida && aspida-mock",
    "licenses:build": "sh scripts/open_source_licenses.sh",
    "about:build": "sh scripts/about_license.sh",
    "dev": "yarn api:build && next",
    "build": "yarn api:build && next build",
    "build:vercel": "yum install zip -y && yarn api:build && yarn licenses:build && yarn build",
    "start": "yarn api:build && yarn licenses:build && next start",
    "type-check": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@aspida/fetch": "^1.5.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@types/cheerio": "^0.22.24",
    "aspida-mock": "^0.8.2",
    "cheerio": "^1.0.0-rc.5",
    "date-fns": "^2.17.0",
    "highlight.js": "^10.6.0",
    "next": "10.0.8-canary.1",
    "notistack": "^1.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "textlint": "^11.8.2",
    "textlint-filter-rule-allowlist": "^2.0.1",
    "textlint-filter-rule-node-types": "^1.1.0",
    "textlint-plugin-html": "^0.2.0",
    "textlint-rule-en-spell": "^1.2.1",
    "textlint-rule-ja-no-space-around-parentheses": "^2.0.2",
    "textlint-rule-ja-space-between-half-and-full-width": "^2.0.2",
    "textlint-rule-preset-ja-technical-writing": "^4.0.1",
    "textlint-rule-preset-jtf-style": "^2.3.6",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/traverse": "^0.6.32",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^0.6.1",
    "typescript": "4.0"
  },
  "license": "MIT"
}
